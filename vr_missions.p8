--objects
function add_bullet()
 add(bullet,{
 bx=x,
 by=y,
 d_bx=4*sin(eyes),
 d_by=4*cos(eyes),
 life=30,
 draw=function(self)
  pset(self.bx,self.by,8)
 end,
 update=function(self)
  self.bx+=self.d_bx
  self.by+=self.d_by 
  self.life-=1
  
  if self.by<play_y2 then
   self.life=0
   end
   
  if self.life<0 then
   del(bullet, self)
  end
  
 end
 })
end

function add_targets()
 add(target,{
 tx=rnd(play_x2), 
 ty=rnd(play_y2),
 tspr=31,
 shot=false,
 draw=function(self)
 spr(self.tspr,self.tx,self.ty)
 --rect(self.tx,self.ty,self.tx+7,self.ty+7,14)
 end,
 update=function(self)
  if self.shot==true then
  del(target,self)
  end
 end,
 hit=function(self)
  
  for b in all(bullet) do
   if box_hit(
 self.tx,self.ty,
 8,8,
 b.bx,b.by,
 0,0) then
 explode(self.tx,self.ty,5,20)
 del(target,self)
   end
  end
 end 
 })
end
--[[to draw and update this table

for o in all(object) do
 o:draw
   or
 o:update
end

]]
