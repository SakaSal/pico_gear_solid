--inti
function _init()
 
 --lock dark palette
 --poke(0x5f2e,1)
 init_head()
 init_playspace()
 init_fov()
 init_player_anim()
 init_hud()
 t=true
 f=false
--better way to bullet
 bullet={}
 target={}
 init_targets()
 sparks = {}
 init_sparks()
end

function init_sparks()
 for i=1,200  do
  add (sparks,{
  x=0,y=0,velx=0,vely=0,
  r=0,alive=false
  })
 end
end

function init_targets()
 for i=1,4 do
 add_targets()
 end
end

function init_player_anim()
 back = f
 
 player={}
 player.sprite = 2
 player.step = 0
 player.animspeed = 5
end

function init_playspace()
 play_x1y1=0
 play_x2=127
 play_y2=108
 corner=15
end

function init_hud()
 edge=127
 item={}
 gun={7,8,23,24}
 knife={9,10,25,26}
 throw_knife={11,12,27,28}
 boot={39,40,55,56}
 nade={41,42,57,58}
 mag={43,44,59,60}
 clock={45,46,61,62}
 head={13,14,29,30}
 item[1]=gun
 item[2]=knife
 item[3]=throw_knife
 item[4]=boot
 item[5]=nade
 item[6]=clock
 item[7]=mag
 item[8]=head
 hud_item=1  
end

function init_head()
 x = 54
 y = 54
 r = 2
end

function init_fov()
fov_r = 19
eyes = .50
end

-->8
--update
function _update()
 update_dir()
 update_player_pos()
 update_hud()
 shoot_gun()
 update_bullets()
end
 
function update_player_pos()
 next_x = 2*sin(eyes)+x
 next_y = 2*cos(eyes)+y
 back_x = 2*sin(eyes+.5)+x
 back_y = 2*cos(eyes+.5)+y
 local input_x = 0
 local input_y = 0
 
 if btn(0) then eyes-=.025 end
 if btn(1) then eyes+=.025 end
 --[[
 if btn(2) then 
 input_x -= x-next_x
 input_y -= y-next_y 
 update_player_anim()
 elseif btn(3) then 
 input_x += back_x-x
 input_y += back_y-y
 update_player_anim() 
 else
 player.sprite = 2
 end
  
 x+=input_x
 y+=input_y
 if (y>play_y2-4) then
 y=play_y2-4
 elseif (y<play_x1y1+2) then
 y=play_x1y1+2
 end
 if (x<play_x1y1+2) then
 x=play_x1y1+2
 elseif (x>125)then
 x=125
 end 
end

function update_player_anim()
 --player sprite animation
 player.step += 1
 if (player.step % player.animspeed == 0) then
 player.sprite += 1
 end
 if (player.sprite > 6) then
 player.sprite = 2
 end
end

function update_hud()
 if btnp(4) then
 hud_item+=1
  if (hud_item>7) then
  hud_item=1
  end
 end
end

function update_dir()
 if (sin(eyes)<0) then
 back=true
 else
 back=false
 end
end

function shoot_gun()
if btnp(5) then
 add_bullet()
 end
end

function update_bullets()
for b in all(bullet) do
 b:update()
 end
 
for t in all(target) do
 t:update()
 end
 
for t in all(target) do
 t:hit()
 end
 
for  i =1,#sparks do
  if sparks[i].alive then
   sparks[i].x += sparks[i].velx/sparks[i].mass
   sparks[i].y += sparks[i].vely/sparks[i].mass
   sparks[i].r -= 0.1
    if sparks[i].r < 0.1 then
     sparks[i].alive = false
   end
  end 
 end
end
