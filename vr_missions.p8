--draw
function _draw()
 cls()
 palt(14,t)
 palt(0,false)
 --activate dark pallet
 --for i=0, 15 do
 --pal(i,i+128,1)
 --end
 draw_playspace()
 draw_fov(x,y)
 draw_player(x,y)
 draw_hud()
 draw_bullets()
 --print(shot,2,2,11)
end

function draw_player(pos_x,pos_y)
 draw_fov(pos_x,pos_y)
 if (back == false)then
 spr(player.sprite,pos_x-4,pos_y-3)
 else
 spr(player.sprite,pos_x-4,pos_y-3,1,1,t)
 end
end

function draw_fov(head_x,head_y)
--define variable for fov radius
 view_x= fov_r*sin(eyes)+head_x
 view_y= fov_r*cos(eyes)+head_y
 line(head_x,head_y,
 fov_r*sin(eyes-.10)+head_x,
 fov_r*cos(eyes-.10)+head_y)
 line(head_x,head_y,
 fov_r*sin(eyes+.10)+head_x,
 fov_r*cos(eyes+.10)+head_y)
 circ(head_x,head_y,fov_r)
 circfill(view_x,view_y,1,8)
--yellow  position tracker
 circfill(next_x,next_y,1,10)
--blue back tracker
 circfill(back_x,back_y,1,1)
end

function draw_playspace()
 rectfill(play_x1y1,play_x1y1,play_x2,play_y2,6)
end

function draw_hud()
 rectfill(play_x1y1,play_y2+1,edge,edge,0)
--item slot
 rectfill(play_x1y1,play_y2+1,18,edge,1)
 spr(corner,14,play_y2-1,1,1,t)
 line(19,play_y2+2,19,126,7)
 spr(corner,14,122,1,1,t,t)
 draw_item(item[hud_item],play_x1y1+2,play_y2+2)

--health
 rectfill(edge-18,play_y2+1,edge,edge,1)
 draw_item(item[8],edge-17,play_y2+3)
 spr(corner,edge-20,play_y2-1)
 line(edge-19,play_y2+1,edge-19,edge,7)
 spr(corner,edge-20,122,1,1,f,t)

   
 spr(corner,play_x1y1-2,play_y2-1)
 line(play_x1y1+1,play_y2+1,123,play_y2+1,7)
 spr(corner,122,play_y2-1,1,1,t)
 line(edge,play_y2+2,edge,126)
 spr(corner,122,122,1,1,t,t)
 line(123,edge,play_x1y1+1,edge)
 spr(corner,play_x1y1-2,122,1,1,f,t)
 line(play_x1y1,play_y2+2,play_x1y1,126)
 print("ammo:▥▥▥",22,111)
end

function draw_item(item,item_x,item_y)
 for i=1,#item do
  spr(item[1],item_x,item_y)
  spr(item[2],item_x+8,item_y) 
  spr(item[3],item_x,item_y+8)
  spr(item[4],item_x+8,item_y+8)
 end 
end

function draw_bullets()
 for b in all(bullet) do
 b:draw()
 end
 
 for t in all(target) do
 t:draw()
 end
 
 for i=1, #sparks do
  if sparks[i].alive then
   circfill(
   sparks[i].x,
   sparks[i].y,
   sparks[i].r,
   5+rnd(5))  
  end
 end
 
end

